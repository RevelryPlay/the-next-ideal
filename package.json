{
  "name": "landing-page",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"node server.js\"",
    "build-all": "node scripts/generate-sitemap.js && vite build && node scripts/generate-static-pages.js",
    "build": "vite build && node scripts/generate-static-pages.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:static": "node scripts/generate-static-pages.js",
    "start": "NODE_ENV=production node server.js",
    "generate:social": "node scripts/generate-social-image.js"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "rss-parser": "^3.13.0",
    "tailwindcss": "^4.1.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "vite": "^7.2.4"
  },
  "devDependencies": {
    "sharp": "^0.34.5"
  }
}
